{
  "name": "emo",
  "version": "0.1.5",
  "description": "An all in one application, includes Reader-a rss reader",
  "main": "./dist/main.js",
  "scripts": {
    "prepare": "husky install",
    "electron": "electronmon .",
    "build": "npm run build:main & npm run build:preload & npm run build:renderer",
    "build:main": "dotenv -e .env.production webpack -- --config ./build/main/webpack.config.prod.ts",
    "build:preload": "dotenv -e .env.production webpack -- --config ./build/preload/webpack.config.prod.ts",
    "build:renderer": "dotenv -e .env.production webpack -- --config ./build/renderer/webpack.config.prod.ts",
    "dev": "npm run dev:main & npm run dev:preload & npm run dev:renderer",
    "dev:main": "dotenv -e .env.development webpack -- --config ./build/main/webpack.config.dev.ts",
    "dev:preload": "dotenv -e .env.development webpack -- --config ./build/preload/webpack.config.dev.ts",
    "dev:renderer": "dotenv -e .env.development webpack serve -- --config ./build/renderer/webpack.config.dev.ts",
    "lint": "npm run eslint && npm run type",
    "eslint": "eslint src/ --ext .ts,.tsx,.js,.jsx",
    "type": "tsc",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "keywords": [
    "electron",
    "emo",
    "rss",
    "reader",
    "rss reader"
  ],
  "author": {
    "name": "creep",
    "email": "creepcn@gmail.com",
    "url": "https://xwcoder.github.io/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xwcoder/emo.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.14",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.11",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-dmg": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "@electron-forge/publisher-github": "^6.4.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.set": "^4.3.9",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/webpack": "^5.28.2",
    "@types/xml2js": "^0.4.12",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "dotenv-cli": "^7.3.0",
    "electron": "^26.3.0",
    "electronmon": "^2.0.2",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.30",
    "postcss-loader": "^7.3.3",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.30.1",
    "@fluentui/react-icons": "^2.0.218",
    "electron-squirrel-startup": "^1.0.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "mobx": "^6.10.2",
    "mobx-react-lite": "^4.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl-universal": "^2.6.21",
    "sqlite3": "^5.1.6",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2"
  },
  "electronmon": {
    "patterns": [
      "src/main/**"
    ]
  }
}
